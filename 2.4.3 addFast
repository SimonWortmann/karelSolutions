void addFast()
{
    repeat(8)
    {
        DoIt();
    }
}

void DoIt()
{ 
    if(onBeeper())
    {
        moveForward();
        if(onBeeper())
        {
            moveForward();
            if(onBeeper())
            {
                moveForward();
                dropBeeper();
                turnRight();
                moveForward();
                turnRight();
                moveForward();
                dropBeeper();
            }
            else
            {
                moveForward();
                turnRight();
                moveForward();
                turnRight();
                moveForward();
                dropBeeper();
            }
        }
        else
        {
            moveForward();
            if(onBeeper())
            {
                moveForward();
                turnRight();
                moveForward();
                turnRight();
                moveForward();
                dropBeeper();
            }
            else
            {
                moveForward();
                dropBeeper();
                turnRight();
                moveForward();
                turnRight();
                moveForward();
            }
        }
    }
    else
    {
        moveForward();
        if(onBeeper())
        {
            moveForward();
            if(onBeeper())
            {
                moveForward();
                turnRight();
                moveForward();
                turnRight();
                moveForward();
                dropBeeper();
            }
            else
            {
                moveForward();
                dropBeeper();
                turnRight();
                moveForward();
                turnRight();
                moveForward();
            }
        }
        else
        {
            moveForward();
            if(onBeeper())
            {
                moveForward();
                dropBeeper();
                turnRight();
                moveForward();
                turnRight();
                moveForward();
            }
            else
            {
                moveForward();
                turnRight();
                moveForward();
                turnRight();
                moveForward();
            }
        }
    }
    moveForward();
    moveForward();
    turnAround();
}
